<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="text" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="~/Content/style/images/favicon.png">
    <title>Gallery Photo</title>
    <link rel="stylesheet" type="text/css" href="~/Content/style/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/style/css/plugins.css">
    <link rel="stylesheet" type="text/css" href="~/Content/style/type/icons.css">
    <link rel="stylesheet" type="text/css" href="~/Content/style/style.css">
    <link href='https://fonts.googleapis.com/css?family=Muli:300,300i,400,400i,600,600i,700,700i' rel='stylesheet' type='text/css'>
</head>
<body>
    <div class="content-wrapper">
        <nav class="navbar solid nav-wrapper-dark inverse-text navbar-expand-lg">
            <div class="container">
                <div class="navbar-header">
                    <div class="navbar-brand"><a href="@Url.Action("Index","Home")"><img src="#" srcset="~/Content/style/images/logo.png 1x, ~/Content/style/images/logo.png 2x" alt="" /></a></div>
                    <div class="navbar-hamburger ml-auto d-lg-none d-xl-none"><button class="hamburger animate" data-toggle="collapse" data-target=".navbar-collapse"><span></span></button></div>
                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav ml-auto">
                        <li class="nav-item">
                            <a class="nav-link" href="@Url.Action("Index","Home")">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="@Url.Action("Add","Photos")">Add Picture</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="@Url.Action("About","Home")">About</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <div class="wrapper light-wrapper">
            @RenderBody()
        </div>
        <footer class="dark-wrapper inverse-text">
            <div class="sub-footer">
                <div class="container inner text-center">
                    <img src="#" srcset="~/Content/style/images/logo.png 1x, ~/Content/style/images/logo.png 2x" alt="" />
                    <div class="space25"></div>
                    <p>© 2018 Gallery Photos.<p>
                </div>
            </div>
        </footer>
    </div>
    <script src="~/Content/style/js/jquery.min.js"></script>
    <script src="~/Content/style/js/popper.min.js"></script>
    <script src="~/Content/style/js/bootstrap.min.js"></script>
    <script src="~/Content/style/revolution/js/jquery.themepunch.tools.min.js"></script>
    <script src="~/Content/style/revolution/js/jquery.themepunch.revolution.min.js"></script>
    <script src="~/Content/style/revolution/revolution-addons/filmstrip/js/revolution.addon.filmstrip.min.js"></script>
    <script src="~/Content/style/revolution/revolution-addons/typewriter/js/revolution.addon.typewriter.min.js"></script>

    <script src="~/Content/style/js/plugins.js"></script>
    <script src="~/Content/style/js/scripts.js"></script>
</body>
</html>